# Django Compressor Configuration
# This file configures minification of CSS and JavaScript files

COMPRESS_ENABLED = true
COMPRESS_OFFLINE = true
COMPRESS_MIMETYPES = (
    'text/html',
    'text/plain',
    'text/xml',
    'text/x-component',
    'text/javascript',
    'text/css',
    'text/cache-manifest',
    'text/vcard',
    'text/vnd.rim.qvcard',
    'image/svg+xml',
    'application/x-javascript',
    'application/javascript',
    'application/x-httpd-eruby',
)
